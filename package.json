{
  "name": "formbucket-client",
  "version": "2.0.0",
  "entropy": "123456",
  "description": "Powerful form handling for static sites",
  "author": "FunctionFoundry",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf public",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:static": "node runMetalSmith && rm -rf public/img/ && cp -r img/ public/",
    "build": "npm run clean && npm run build:static && npm run build:webpack",
    "start": "node devServer.js",
    "start:static": "cd public && http-server . -p 3000",
    "lint": "eslint src",
    "deploy:www-dev": "rsync -apvz -e \"ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null\" --progress ~/formbucket-client/public/ whqops@10.8.0.1:/srv/www/www-dev.formbucket.com/public/"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^5.0.4",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "handlebars": "^4.0.5",
    "highlight.js": "^9.6.0",
    "json-loader": "^0.5.4",
    "koa": "^1.2.1",
    "koa-accesslog": "0.0.2",
    "koa-front-matter-views": "^2.0.4",
    "koa-route": "^2.4.2",
    "koa-sass": "^1.0.1",
    "koa-send": "^3.2.0",
    "koa-static": "^2.0.0",
    "koa-webpack-dev-middleware": "^1.2.2",
    "metalsmith": "^2.1.0",
    "metalsmith-in-place": "^1.4.4",
    "metalsmith-layouts": "^1.4.1",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-remarkable": "git+https://github.com/mulyoved/metalsmith-remarkable.git",
    "node-sass": "^3.7.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
