    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "handlebars": "^4.0.5",
    "highlight.js": "^9.6.0",
    "json-loader": "^0.5.4",
    "koa": "^1.2.1",
    "koa-accesslog": "0.0.2",
    "koa-front-matter-views": "^2.0.4",
