{
  "name": "formbucket-client",
  "version": "0.3.0",
  "description": "Powerful form handling for static sites",
  "author": "FunctionFoundry",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "copy-to-server": "cp -r public/ ../formbucket-server/public && cp dist/formbucket.j* ../formbucket-server/public/js/ && cp dist/formbucket.cs* ../formbucket-server/public/css/",
    "start": "node devServer.js",
    "lint": "eslint src",
    "g": "npm run build && npm run copy-to-server"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "formbucket-common": "^0.6.10",
    "functionfoundry": "^0.6.5",
    "highlight.js": "^9.0.0",
    "history": "^1.17.0",
    "immutable": "^3.7.5",
    "moment": "^2.10.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-fontawesome": "^0.3.3",
    "react-remarkable": "^1.1.1",
    "react-router": "^1.0.3",
    "react-set-animate": "0.0.15",
    "react-static-container": "^1.0.0",
    "sweetflux": "^0.7.0"
  }
}
